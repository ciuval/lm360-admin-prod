    "lint:soft": "eslint --ext .js,.jsx src --ignore-path .eslintignore || exit 0",
    "format": "prettier --write \"src/**/*.{js,jsx,css,html,json}\"",
    "check": "npm run build"
  },
  "dependencies": {
