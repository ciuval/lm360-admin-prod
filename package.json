    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "dotenv": "^16.4.5",
    "framer-motion": "^12.23.12",
    "nanoid": "^5.1.6",
    "nodemailer": "^6.9.13",
    "plausible-tracker": "^0.3.9",
    "raw-body": "^2.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-qr-code": "^2.0.18",
